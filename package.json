{
  "name": "@u3u/prettier-config",
  "version": "4.1.2",
  "description": "ðŸŽ¨ The most perfect Prettier config",
  "keywords": [
    "prettier-config",
    "prettier-preset"
  ],
  "repository": {
    "url": "https://github.com/u3u/prettier-config.git"
  },
  "license": "MIT",
  "author": "u3u <qwq@qwq.cat> (https://github.com/u3u)",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./*": {
      "import": {
        "types": "./dist/*.d.mts",
        "default": "./dist/*.mjs"
      },
      "require": {
        "types": "./dist/*.d.ts",
        "default": "./dist/*.js"
      }
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup && fix-tsup-cjs",
    "format": "prettier --write .",
    "lint": "eslint --fix .",
    "prepare": "pnpm build",
    "test": "vitest"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged --concurrent false"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix"
    ],
    "*": [
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@prettier/plugin-php": "^0.21.0",
    "@prettier/plugin-xml": "^3.2.2",
    "@shufo/prettier-plugin-blade": "^1.13.3",
    "deepmerge": "^4.3.1",
    "local-pkg": "^0.4.3",
    "prettier-plugin-astro": "^0.12.1",
    "prettier-plugin-ini": "^1.1.0",
    "prettier-plugin-jsdoc": "^1.1.1",
    "prettier-plugin-nginx": "^1.0.3",
    "prettier-plugin-packagejson": "^2.4.6",
    "prettier-plugin-prisma": "^5.0.0",
    "prettier-plugin-sh": "^0.13.1",
    "prettier-plugin-sort-json": "^3.1.0",
    "prettier-plugin-sql": "^0.15.1",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "remeda": "^1.28.0"
  },
  "devDependencies": {
    "@total-typescript/ts-reset": "^0.5.1",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.18.8",
    "@u3u/eslint-config": "^2.12.1",
    "@u3u/prettier-config": "link:",
    "dedent": "^1.5.1",
    "eslint": "^8.52.0",
    "execa": "^8.0.1",
    "fix-tsup-cjs": "^1.2.0",
    "globby": "^13.2.2",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "simple-git-hooks": "^2.9.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  },
  "peerDependencies": {
    "prettier": ">=3.0.0"
  },
  "packageManager": "pnpm@8.10.2",
  "publishConfig": {
    "access": "public"
  },
  "pnpm": {
    "updateConfig": {
      "ignoreDependencies": [
        "@types/node",
        "local-pkg"
      ]
    }
  }
}
