{
  "name": "@u3u/prettier-config",
  "version": "1.0.2",
  "description": "My Prettier config",
  "keywords": [
    "prettier-config"
  ],
  "repository": {
    "url": "https://github.com/u3u/prettier-config.git"
  },
  "license": "MIT",
  "author": "u3u <qwq@qwq.cat> (https://github.com/u3u)",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "require": "./lib/index.js",
      "import": "./dist/index.mjs"
    },
    "./base": {
      "types": "./lib/base.d.ts",
      "require": "./lib/base.js",
      "import": "./dist/base.mjs"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "require": "./dist/utils/index.js",
      "import": "./dist/utils/index.mjs"
    }
  },
  "main": "./lib/index.js",
  "module": "./dist/index.mjs",
  "types": "./lib/index.d.ts",
  "typesVersions": {
    "*": {
      "base": [
        "./lib/base.d.ts"
      ],
      "utils": [
        "./dist/utils/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "build": "tsup src/*.ts src/utils/*.ts --format cjs,esm --dts-resolve --no-splitting --shims --clean",
    "format": "prettier --write .",
    "preinstall": "npx only-allow pnpm",
    "prepare": "pnpm build"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,md,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^3.7.2",
    "@prettier/plugin-php": "^0.19.4",
    "@prettier/plugin-xml": "^2.2.0",
    "@shufo/prettier-plugin-blade": "^1.8.12",
    "deepmerge": "^4.3.1",
    "prettier": "^2.8.8",
    "prettier-plugin-ini": "^1.1.0",
    "prettier-plugin-jsdoc": "^0.4.2",
    "prettier-plugin-nginx": "^1.0.3",
    "prettier-plugin-packagejson": "^2.4.3",
    "prettier-plugin-sh": "^0.12.8",
    "prettier-plugin-sort-json": "^1.0.0",
    "prettier-plugin-sql": "^0.14.0"
  },
  "devDependencies": {
    "@tsconfig/node18": "^2.0.0",
    "@types/node": "^18.16.0",
    "@u3u/prettier-config": "link:",
    "lint-staged": "^13.2.1",
    "simple-git-hooks": "^2.8.1",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "prettier": "^2.8.7"
  },
  "packageManager": "pnpm@8.3.1",
  "publishConfig": {
    "access": "public"
  }
}
