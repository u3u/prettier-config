{
  "name": "@u3u/prettier-config",
  "version": "3.1.0",
  "description": "My Prettier config",
  "keywords": [
    "prettier-config"
  ],
  "repository": {
    "url": "https://github.com/u3u/prettier-config.git"
  },
  "license": "MIT",
  "author": "u3u <qwq@qwq.cat> (https://github.com/u3u)",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./*": {
      "import": {
        "types": "./dist/*.d.mts",
        "default": "./dist/*.mjs"
      },
      "require": {
        "types": "./dist/*.d.ts",
        "default": "./dist/*.js"
      }
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup && fix-tsup-cjs",
    "format": "prettier --write .",
    "lint": "eslint --fix .",
    "prepare": "pnpm build",
    "test": "vitest"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged --concurrent false"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix"
    ],
    "*": [
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.0",
    "@prettier/plugin-php": "^0.19.6",
    "@prettier/plugin-xml": "^2.2.0",
    "@shufo/prettier-plugin-blade": "^1.8.13",
    "@types/prettier": "^2.7.3",
    "deepmerge": "^4.3.1",
    "local-pkg": "^0.4.3",
    "prettier-plugin-ini": "^1.1.0",
    "prettier-plugin-jsdoc": "^0.4.2",
    "prettier-plugin-nginx": "^1.0.3",
    "prettier-plugin-packagejson": "^2.4.5",
    "prettier-plugin-prisma": "^5.0.0",
    "prettier-plugin-sh": "^0.12.8",
    "prettier-plugin-sort-json": "^2.0.0",
    "prettier-plugin-sql": "^0.14.0",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "prettier-plugin-twin.macro": "1.0.14",
    "remeda": "^1.24.0"
  },
  "devDependencies": {
    "@total-typescript/ts-reset": "^0.4.2",
    "@tsconfig/node18": "^18.2.0",
    "@types/node": "^18.17.4",
    "@u3u/eslint-config": "^2.9.2",
    "@u3u/prettier-config": "link:",
    "eslint": "^8.47.0",
    "fix-tsup-cjs": "^1.2.0",
    "globby": "^13.2.2",
    "lint-staged": "^14.0.0",
    "prettier": "^2.8.8",
    "simple-git-hooks": "^2.9.0",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6",
    "vitest": "^0.34.1"
  },
  "peerDependencies": {
    "prettier": "^2.8.8"
  },
  "packageManager": "pnpm@8.6.12",
  "publishConfig": {
    "access": "public"
  },
  "pnpm": {
    "updateConfig": {
      "ignoreDependencies": [
        "@prettier/plugin-xml",
        "@shufo/prettier-plugin-blade",
        "prettier-plugin-jsdoc",
        "prettier-plugin-sh",
        "prettier-plugin-sql"
      ]
    }
  }
}
