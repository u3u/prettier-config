{
  "name": "@u3u/prettier-config",
  "version": "1.0.5",
  "description": "My Prettier config",
  "keywords": [
    "prettier-config"
  ],
  "repository": {
    "url": "https://github.com/u3u/prettier-config.git"
  },
  "license": "MIT",
  "author": "u3u <qwq@qwq.cat> (https://github.com/u3u)",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./base": {
      "types": "./dist/base.d.ts",
      "require": "./dist/base.js",
      "import": "./dist/base.mjs"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "require": "./dist/utils.js",
      "import": "./dist/utils.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup && fix-tsup-cjs",
    "format": "prettier --write .",
    "lint": "eslint --fix .",
    "prepare": "pnpm build"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged --concurrent false"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix"
    ],
    "*": [
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^3.7.2",
    "@prettier/plugin-php": "^0.19.4",
    "@prettier/plugin-xml": "^2.2.0",
    "@shufo/prettier-plugin-blade": "^1.8.12",
    "deepmerge": "^4.3.1",
    "prettier-plugin-ini": "^1.1.0",
    "prettier-plugin-jsdoc": "^0.4.2",
    "prettier-plugin-nginx": "^1.0.3",
    "prettier-plugin-packagejson": "^2.4.3",
    "prettier-plugin-sh": "^0.12.8",
    "prettier-plugin-sort-json": "^1.0.0",
    "prettier-plugin-sql": "^0.14.0"
  },
  "devDependencies": {
    "@tsconfig/node18": "^2.0.1",
    "@types/node": "^18.16.13",
    "@u3u/eslint-config": "^2.2.2",
    "@u3u/prettier-config": "link:",
    "eslint": "^8.41.0",
    "fix-tsup-cjs": "^1.1.1",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "simple-git-hooks": "^2.8.1",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "prettier": "^2.8.8"
  },
  "packageManager": "pnpm@8.5.1",
  "publishConfig": {
    "access": "public"
  }
}
